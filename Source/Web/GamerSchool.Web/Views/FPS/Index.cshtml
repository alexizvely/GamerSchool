@using GamerSchool.Common
@model GamerSchool.Web.ViewModels.FPS.FPSIndexViewModel
@{
    ViewBag.Title = "FPS";
}
<h2 class="text-center">FPS</h2>
<div class="container well">
    @if (Request.IsAuthenticated && (User.IsInRole(ApplicationRoles.Admin) || User.IsInRole(ApplicationRoles.Designer)))
    {
        <div calss="row text-center">
            <div class="col-md-12">
                <a href="/Users/FPS/Create" class="btn btn-md btn-primary full-width">Create new Design</a>
            </div>
        </div>
    }
    @Html.Partial("../Tutorial/_TutorialPagedPartial", Model)
</div>
